fn basic(lexes){
  mut turi_code = {
      for each lex in lexes list {
          if lex.atLineStart {
              continue "mut" if lex.text == "LET";
              continue "while" if lex.text == "WHILE";
              continue "}" if lex.text == "WEND";
              continue "for" if lex.text == "FOR";
              continue "if" if lex.text == "IF";
              continue "}" if lex.text == "ENDIF";
              continue "println" if lex.text == "PRINT";
          }
          continue "{" if lex.text == "DO";
          continue "} else {" if lex.text == "ELSE";
          continue "{" if lex.text == "THEN";

          lex;
      }
  }
  turi_code
}

fn LIST(lexes){
    for each lex in lexes {
        if lex.atLineStart {
            println "";
        }
        print lex.text, " "
    }
}


